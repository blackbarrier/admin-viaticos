<form method="post" action="{{ path('app_usuario_delete', {'id': usuario.id}) }}" onsubmit="checkIfWanted()">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ usuario.id) }}">
    <button class="btn btn-inverse btn-inverse-jag btn-outline-inverse"><i class="fa fa-strash"></i>&nbsp; Borrar</button>
</form>
